{
  "$description": "DTCG 2025.10 compliant example. This file uses strict DTCG format without Variable Design Standard (VDS) extensions like modes in $value.",
  "color": {
    "base": {
      "gray": {
        "100": {
          "$type": "color",
          "$value": {
            "colorSpace": "srgb",
            "components": [0.96, 0.96, 0.96],
            "hex": "#f5f5f5"
          }
        },
        "900": {
          "$type": "color",
          "$value": {
            "colorSpace": "srgb",
            "components": [0.1, 0.1, 0.1],
            "hex": "#1a1a1a"
          }
        }
      },
      "primary": {
        "$type": "color",
        "$value": {
          "colorSpace": "srgb",
          "components": [0.278431, 0.388235, 0.74902],
          "hex": "#4763BF"
        },
        "$extensions": {
          "com.variables-contract.version": "0.5.0"
        }
      },
      "secondary": {
        "$type": "color",
        "$value": {
          "colorSpace": "srgb",
          "components": [0.4, 0.4, 0.4],
          "hex": "#666666"
        }
      }
    },
    "semantic": {
      "$extends": "{color.base}",
      "text": {
        "primary": {
          "$type": "color",
          "$value": "{color.base.gray.900}"
        },
        "secondary": {
          "$type": "color",
          "$value": "{color.base.secondary}"
        }
      },
      "surface": {
        "default": {
          "$type": "color",
          "$value": {
            "colorSpace": "srgb",
            "components": [1, 1, 1],
            "hex": "#ffffff"
          }
        }
      }
    }
  },
  "spacing": {
    "base": {
      "small": {
        "$type": "dimension",
        "$value": {
          "value": 8,
          "unit": "px"
        }
      },
      "medium": {
        "$type": "dimension",
        "$value": {
          "value": 16,
          "unit": "px"
        }
      },
      "large": {
        "$type": "dimension",
        "$value": {
          "value": 24,
          "unit": "px"
        }
      }
    },
    "semantic": {
      "component": {
        "gap": {
          "sm": {
            "$type": "dimension",
            "$value": "{spacing.base.small}"
          },
          "md": {
            "$type": "dimension",
            "$value": "{spacing.base.medium}"
          }
        }
      }
    }
  },
  "font": {
    "family": {
      "primary": {
        "$type": "fontFamily",
        "$value": "Roboto"
      }
    },
    "weight": {
      "normal": {
        "$type": "fontWeight",
        "$value": 400
      },
      "bold": {
        "$type": "fontWeight",
        "$value": 700
      }
    },
    "size": {
      "base": {
        "$type": "dimension",
        "$value": {
          "value": 16,
          "unit": "px"
        }
      },
      "lg": {
        "$type": "dimension",
        "$value": {
          "value": 24,
          "unit": "px"
        }
      }
    }
  },
  "duration": {
    "fast": {
      "$type": "duration",
      "$value": {
        "value": 200,
        "unit": "ms"
      }
    },
    "slow": {
      "$type": "duration",
      "$value": {
        "value": 500,
        "unit": "ms"
      }
    }
  },
  "easing": {
    "default": {
      "$type": "cubicBezier",
      "$value": [0.25, 0.1, 0.25, 1]
    },
    "ease-in": {
      "$type": "cubicBezier",
      "$value": [0.42, 0, 1, 1]
    }
  },
  "border": {
    "default": {
      "$type": "border",
      "$value": {
        "width": {
          "value": 1,
          "unit": "px"
        },
        "color": {
          "colorSpace": "srgb",
          "components": [0.88, 0.88, 0.88],
          "hex": "#e0e0e0"
        },
        "style": "solid"
      }
    },
    "thick": {
      "$type": "border",
      "$value": {
        "width": {
          "value": 2,
          "unit": "px"
        },
        "color": "{color.semantic.text.secondary}",
        "style": "solid"
      }
    }
  },
  "transition": {
    "default": {
      "$type": "transition",
      "$value": {
        "duration": "{duration.fast}",
        "delay": {
          "value": 0,
          "unit": "ms"
        },
        "timingFunction": "{easing.default}"
      }
    }
  },
  "shadow": {
    "small": {
      "$type": "shadow",
      "$value": {
        "color": {
          "colorSpace": "srgb",
          "components": [0, 0, 0],
          "alpha": 0.1
        },
        "offsetX": {
          "value": 0,
          "unit": "px"
        },
        "offsetY": {
          "value": 2,
          "unit": "px"
        },
        "blur": {
          "value": 4,
          "unit": "px"
        },
        "spread": {
          "value": 0,
          "unit": "px"
        }
      }
    },
    "large": {
      "$type": "shadow",
      "$value": {
        "color": {
          "colorSpace": "srgb",
          "components": [0, 0, 0],
          "alpha": 0.2
        },
        "offsetX": {
          "value": 0,
          "unit": "px"
        },
        "offsetY": {
          "value": 8,
          "unit": "px"
        },
        "blur": {
          "value": 16,
          "unit": "px"
        },
        "spread": {
          "value": 0,
          "unit": "px"
        }
      }
    }
  },
  "gradient": {
    "primary": {
      "$type": "gradient",
      "$value": [
        {
          "color": "{color.base.primary}",
          "position": 0
        },
        {
          "color": {
            "colorSpace": "srgb",
            "components": [0, 0.27, 0.6],
            "hex": "#004499"
          },
          "position": 1
        }
      ]
    }
  },
  "typography": {
    "heading": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{font.family.primary}",
        "fontSize": "{font.size.lg}",
        "fontWeight": "{font.weight.bold}",
        "letterSpacing": {
          "value": 0,
          "unit": "px"
        },
        "lineHeight": 1.5
      }
    },
    "body": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{font.family.primary}",
        "fontSize": "{font.size.base}",
        "fontWeight": "{font.weight.normal}",
        "letterSpacing": {
          "value": 0,
          "unit": "px"
        },
        "lineHeight": 1.2
      }
    }
  },
  "component": {
    "button": {
      "color": {
        "background": {
          "default": {
            "$type": "color",
            "$value": "{color.semantic.surface.default}"
          }
        },
        "text": {
          "default": {
            "$type": "color",
            "$value": "{color.semantic.text.primary}"
          }
        }
      },
      "border": {
        "default": {
          "$type": "border",
          "$value": "{border.default}"
        }
      },
      "spacing": {
        "padding": {
          "x": {
            "$type": "dimension",
            "$value": "{spacing.semantic.component.gap.md}"
          },
          "y": {
            "$type": "dimension",
            "$value": "{spacing.semantic.component.gap.sm}"
          }
        }
      }
    }
  }
}
